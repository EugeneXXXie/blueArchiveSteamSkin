{
    "Steam-WebKit": "./css/webkit.css",
    "Patches": [
        {
            "MatchRegexString": ".*http.*steam.*",
            "TargetJs": "./js/webkit.js"
        },
        {
            "MatchRegexString": "^Steam$",
            "TargetCss": "./css/steam.css",
            "TargetJs": "./js/steam.js"
        },
        {
            "MatchRegexString": ".friendsui-container",
            "TargetCss": "./css/friends.custom.css",
            "TargetJs": "./js/friends.custom.css.js"
        }
    ],
    "UseDefaultPatches": true,
    "author": "EugeneXXXie",
    "description": "Steam Skin Template",
    "name": "BlueArchive",
    "version": "1.0.1",
    "github": {
        "owner": "EugeneXXXie",
        "repo_name": "blueArchiveSteamSkin"
    },
    "discord_support": {
        "inviteCodeExcludingLink": "None"
    },
    "header_image": "http://ayachinene.top:11080/assets/favicon.8b1ea97d.ico",
    "splash_image": "http://ayachinene.top:11080/assets/favicon.8b1ea97d.ico"
}

